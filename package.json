{
  "name": "project-task-cafe",
  "description": "Template for module Advanced JavaScript and TypeScript",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --open",
    "lint": "eslint --ext .js ./",
    "lint:fix": "eslint --fix --ext .js ./",
    "format": "prettier --write \"./**/*.{js,jsx,json,css}\"",
    "build": "vite build --base=/project-task-cafe/",
    "preview": "vite preview --open",
    "buildpreview": "vite build && vite preview --open"
  },
  "keywords": [
    "javascript",
    "template",
    "postcss",
    "eslint",
    "prettier",
    "vite"
  ],
  "author": "Yaroslav Kosytsia <yaroslavkosytsia@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "cssnano": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "glob": "^8.1.0",
    "postcss": "^8.4.38",
    "postcss-nesting": "^12.1.5",
    "prettier": "^3.3.2",
    "vite": "5.3.2",
    "vite-plugin-eslint": "^1.8.1"
  },
  "dependencies": {
    "izitoast": "^1.4.0",
    "the-new-css-reset": "^1.11.2",
    "vite-plugin-html-inject": "^1.1.2"
  }
}
